[user]
  name = Alex Tareshawty
  email = alex@tareshawty.com
[alias]
  aa = add -A
  amend = commit --amend --no-edit
  br = branch
  co = checkout
  cob = checkout -b
  com = checkout main
  cp = cherry-pick
  d = diff
  ds = diff --staged
  grepc = grep --ignore-case
  head = !git log -n 1 | sed -n 1p | sed -e 's/commit //' | tr -d '\n' | pbcopy
  hist = log --graph
  logfp = log --first-parent
  st = status
  wip = !git add -A && git commit -m "wip"
